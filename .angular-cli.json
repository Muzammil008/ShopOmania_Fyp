{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "shop-o-mania"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",

      "prefix": "app",
      "styles": [
        "styles.css",
		
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/sweetalert2/dist/sweetalert2.min.css",
		"../node_modules/angular2-busy/build/style/busy.css",
		"./assets/bower_components/font-awesome/css/font-awesome.min.css",
		"./assets/bower_components/Ionicons/css/ionicons.min.css",
		"./assets/dist/css/AdminLTE.min.css",
		"./assets/dist/css/skins/skin-blue.min.css",


		
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/jquery-ui586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/jquery.owl.carousel586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/jquery.swiper586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/bc.toggle.scss586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/bc.global.scss586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/bc.style.scss586b.css",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/bc.green.scss586b.css"
		
		
		
		
		
		

	
      ],
      "scripts": [
      "../node_modules/jquery/dist/jquery.min.js",

          "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js",
        "../node_modules/crypto-js/crypto.js",
		"./cdn.shopify.com/s/files/1/1067/5616/t/5/assets/test.js"
			
	
		
			
       
		
		
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}